{
  "name": "rsuite-with-electron",
  "version": "1.0.0",
  "description": "An example that use Rsuite in Electron application",
  "main": "main.js",
  "scripts": {
    "start": "export ELECTRON_START_URL=http://localhost:${npm_package_config_port} && concurrently -k \"npm:dev.*\"",
    "dev.electron": "electron .",
    "dev.webpack": "webpack-dev-server --mode development --progress --hot",
    "build": "npm run build.webpack && npm run build.electron",
    "build.webpack": "rm -rf dist && webpack --mode production --progress  --display-modules",
    "build.electron": "rm -rf build && electron-builder -c electron-builder.json",
    "dist": "electron-builder"
  },
  "config": {
    "port": "4000"
  },
  "keywords": [
    "rsuite",
    "electron",
    "react"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "concurrently": "^5.2.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "rsuite": "^4.4.1"
  }
}
